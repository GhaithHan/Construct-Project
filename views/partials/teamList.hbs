<div class="teamList">
    <div class="teamListTitle">
        <h2>Your Future Collaborators</h2>
    </div>

    <div class="teamListList">
        <ul>
            {{#each memberTeamAdded}}
            <div class="teamListEach">
                <li>
                    <div>
                        <h3>{{this.firstName}} {{this.lastName}}</h3>
                    </div>
                    <div>
                    <h4>{{this.job}}</h4>
                    </div>
                    <div>
                    <a href="mailto:{{this.email}}">Email</a>
                    <p>-----------</p>
                    </div>
                </li>
            {{/each}}
            </div>

        </ul>
    </div>
</div>
